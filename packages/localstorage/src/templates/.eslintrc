extends:
  - "@divmain/eslint-config-defaults/configurations/walmart/es5-browser"

globals:
  r: true
  CACHE_IDS: true

rules:
  no-magic-numbers: 0
  no-unused-expressions: 0
  no-extra-parens: 0
  no-unused-vars: 0
  max-len: 0
  valid-jsdoc: 0
  no-return-assign: 0
  func-style: 0
  space-before-function-paren: [2, { "anonymous": "always", "named": "always" }]
  no-new-func: 0
